#include <iostream>
using namespace std;

int main() {
    //Write your code here
  int start, end, steps, celsius;
    cin>>start>>end>>steps;
    int i;
    for(i=start;i<end;i+=steps){
        celsius = (i-32)*5/9;
         cout<<i<<" "<<celsius<<endl;
    }


    return 0;
}