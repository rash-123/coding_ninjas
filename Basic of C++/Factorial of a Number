#include <iostream>
using namespace std;

int main() {
    //Write your code here
    int n, fat = 1;
    cin>>n;
    if(n==0){
        cout<<"1"<<endl;
    }
    else if(n<0){
        cout<<"Error"<<endl;
    }
    else {
    for(int i=1;i<=n;i++){
        fat = fat*i;
    }
    cout<<fat<<endl;
    }
    

    return 0;
}