#include <iostream>
using namespace std;

int main() {
    //Write your code here
    int Principal;
    cin>>Principal;
    double Rate;
    cin>>Rate;
    int Time;
    cin>>Time;
    int SimpleInterest = Principal*Rate*Time/100;
    cout<< SimpleInterest <<endl;
    

    return 0;
}